<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stussy.stussclone20220929junho.repository.AccountRepository"> <!-- 경로 -->

    <insert id="save" parameterType="com.stussy.stussclone20220929junho.domain.User"> <!--데이터베이스 정보-->
        insert into
            user_mst
        values(
            0,
            #{username},
            #{oauth_username},
            #{name},
            #{email},
            #{provider},
            #{role_id},
            now(),
            now()
        )
    </insert>
</mapper>